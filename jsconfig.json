{
    "compilerOptions": {
        "baseUrl": "src",
        "paths": {
            "@components/*": [
                "./src/components/*"
            ],
            "@constants/*": [
                "./src/constants/*"
            ],
            "@css/*": [
                "./src/css/*"
            ],
            "@hooks/*": [
                "./src/hooks/*"
            ],
            "@/*": [
                "./src/*"
            ]
        }
    }
}
