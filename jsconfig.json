{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@components/*": [
                "./src/components/*"
            ],
            "@constants/*": [
                "./src/constants/*"
            ],
            "@css/*": [
                "./src/css/*"
            ],
            "@hooks/*": [
                "./src/hooks/*"
            ],
            "@pages/*": [
                "./src/pages/*"
            ],
            "@widgets/*": [
                "./src/widgets/*"
            ],
            "@/*": [
                "./src/*"
            ]
        }
    }
}